rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /Acc/{accID} {
      allow create: if request.auth.uid != null;
      allow read, update: if request.auth.uid == accID;
    }
    match /mymovie/{mymovieID} {
      allow read, create, update: if request.auth.uid == mymovieID;
    }
  }
}